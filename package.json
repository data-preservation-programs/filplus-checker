{
  "name": "@data-preservation-programs/filplus-checker",
  "version": "0.0.1",
  "description": "Checker tool for Fil+ datacap applications",
  "scripts": {
    "build": "tsc",
    "test": "jasmine-ts --config=jasmine.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./dist/src/app.js",
    "lint": "eslint --ext ts --fix src/",
    "lint:nofix": "eslint --ext ts src/",
    "release": "rm -rf dist && npm run build && cp package*.json dist && cd dist && npm ci --omit=dev && strip ./node_modules/canvas/build/Release/librsvg-2.so.2 && rm -rf ./node_modules/chart.js/dist/docs && zip -r index.zip ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/data-preservation-programs/filplus-checker.git"
  },
  "keywords": [
    "fil+",
    "datacap",
    "filecoin",
    "github",
    "checker"
  ],
  "license": "MIT",
  "homepage": "https://github.com/data-preservation-programs/filplus-checker",
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^18.11.9",
    "@types/node-emoji": "^1.8.2",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine": "^3.99.0",
    "jasmine-reporters": "^2.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@probot/adapter-aws-lambda-serverless": "^3.0.1",
    "async-retry": "^1.3.3",
    "axios": "^1.2.1",
    "canvas": "^2.10.2",
    "chart.js": "^3.9.1",
    "chartjs-chart-geo": "3.9.0",
    "chartjs-plugin-datalabels": "^2.1.0",
    "markdown-table": "^2.0.0",
    "multiaddr": "^10.0.1",
    "nock": "^13.2.9",
    "node-emoji": "^1.11.0",
    "ordinal": "^1.0.3",
    "pg": "^8.8.0",
    "probot": "^12.2.8",
    "smee-client": "^1.2.2",
    "xbytes": "^1.8.0"
  }
}
